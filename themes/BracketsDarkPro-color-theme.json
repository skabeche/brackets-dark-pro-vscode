{
  "name": "Brackets Dark Pro",
  "colors": {
    "editor.background": "#1d1f21",
    "editorCursor.foreground": "#c5c8c6",
    "editor.foreground": "#dddddd",
    "editor.lineHighlightBackground": "#2f2f2f",
    "editor.selectionBackground": "#0050a0",
    "activityBarBadge.background": "#007acc",
    "sideBarTitle.foreground": "#bbbbbb"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#767676"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#dddddd"
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#d89333"
      }
    },
    // {
    //   "name": "Invalid",
    //   "scope": [
    //     "invalid",
    //     "invalid.illegal"
    //   ],
    //   "settings": {
    //     "foreground": "#FF5370"
    //   }
    // },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#6c9ef8"
      }
    },
    {
      "name": "Keyword operator",
      "scope": [
        "keyword.operator",
      ],
      "settings": {
        "foreground": "#dddddd"
      }
    },
    {
